{
  "name": "chroma-js",
  "description": "JavaScript library for color conversions",
  "version": "1.3.4",
  "author": "Gregor Aisch",
  "homepage": "https://github.com/gka/chroma.js",
  "main": "chroma.js",
  "module": "chroma.es.js",
  "keywords": [
    "color"
  ],
  "files": [
    "chroma.js",
    "chroma.es.js",
    "chroma.min.js"
  ],
  "maintainers": [
    {
      "name": "Gregor Aisch",
      "email": "mail@driven-by-data.net",
      "web": "http://driven-by-data.net"
    }
  ],
  "bugs": "https://github.com/gka/chroma.js/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/gka/chroma.js.git"
  },
  "scripts": {
    "build": "node rollup.node && rollup -c && uglifyjs chroma.js --comments -c -m -o chroma.min.js",
    "dev": "rollup -c -w",
    "test": "./node_modules/vows/bin/vows --dot-matrix"
  },
  "devDependencies": {
    "es6-shim": "^0.18.0",
    "rollup": "^0.43.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^4.0.0",
    "uglify-js": "2.x",
    "vows": "0.8.x"
  },
  "spm": {
    "main": "chroma.js",
    "ignore": [
      "src",
      "doc",
      "test"
    ]
  }
}
